{
  "rabbit_version": "3.8.9",
  "rabbitmq_version": "3.8.9",
  "users": [
    {
      "name": "guest",
      "password_hash": "guest",
      "hashing_algorithm": "rabbit_password_hashing_sha256",
      "tags": "administrator"
    }
  ],
  "vhosts": [
    {
      "name": "/"
    }
  ],
  "permissions": [
    {
      "user": "guest",
      "vhost": "/",
      "configure": ".*",
      "write": ".*",
      "read": ".*"
    }
  ],
  "topic_permission" : [
    {
      "user": "guest",
      "vhost":"/",
      "exchange":"",
      "write":".*",
      "read":".*"
    }
  ],
  "parameters": [],
  "global_parameters": [
    {
      "name": "cluster_name",
      "value": "rabbit@my-rabbit"
    }
  ],
  "policies": [],
  "queues": [
    {
      "name": "loanEvaluation.customer",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type" : "classic"
      }
    },
    {
      "name": "loanEvaluation.loanApplication",
      "vhost": "/",
      "durable": true,
      "auto_delete": false,
      "arguments": {
        "x-queue-type" : "classic"
      }
    }
  ],
  "exchanges": [
    {
      "name": "topic.exchange",
      "vhost": "/",
      "type": "topic",
      "durable": true,
      "auto_delete": false,
      "internal": false,
      "arguments": {}
    }
  ],
  "bindings": [
    {
      "source": "topic.exchange",
      "vhost": "/",
      "destination": "loanEvaluation.customer",
      "destination_type": "queue",
      "routing_key": "loanEvaluation.customer",
      "arguments": {}
    },
    {
      "source": "topic.exchange",
      "vhost": "/",
      "destination": "loanEvaluation.loanApplication",
      "destination_type": "queue",
      "routing_key": "loanEvaluation.loanApplication",
      "arguments": {}
    }
  ]
}